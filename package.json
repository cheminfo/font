{
  "name": "cheminfo-font",
  "version": "0.34.1",
  "description": "Cheminfo font and CSS to include it in web pages",
  "main": "dist/cheminfo.css",
  "files": [
    "dist",
    "svg",
    "lib-react",
    "react.js"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run clean && npm run build-svg && npm run build-react",
    "build-react": "npm run build-react-tsx && npm run build-react-js",
    "build-react-tsx": "svgr --icon --typescript --title-prop --index-template svgr-index-template.js --out-dir lib-react-tsx svg",
    "build-react-js": "tsc || echo 0",
    "build-svg": "node build-svg.js",
    "clean": "rimraf dist lib-react lib-react-tsx svg",
    "test": "echo 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/font.git"
  },
  "author": "MichaÃ«l Zasso",
  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://github.com/cheminfo/font/issues"
  },
  "homepage": "https://github.com/cheminfo/font#readme",
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@types/react": "^17.0.0",
    "btoa": "^1.2.1",
    "fs-extra": "^9.0.1",
    "rimraf": "^3.0.2",
    "svgo": "^1.3.2",
    "typescript": "^4.1.3"
  }
}
